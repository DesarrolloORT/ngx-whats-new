{
  "name": "ngx-whats-new-lib",
  "version": "1.1.2",
  "type": "module",
  "scripts": {
    "start": "ng serve",
    "build": "ng build ngx-whats-new",
    "build:app": "ng build test-app",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "npm run test -- --ci --silent --maxWorkers=3 --no-watchman",
    "test:sonar": "npm run test -- --coverage --ci --no-watchman --coverageReporters=lcov --maxWorkers=3 --silent --clearMocks",
    "test:snapshot": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run lint:check && npm run format:check && npm run test && npm run build:app && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.8",
    "@angular/cdk": "^19.2.11",
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/forms": "^19.2.8",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/router": "^19.2.8",
    "ngx-whats-new": "file:dist/ngx-whats-new",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular-eslint/schematics": "^19.3.0",
    "@angular-eslint/template-parser": "^19.3.0",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.8",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@typescript-eslint/types": "^8.31.0",
    "@typescript-eslint/utils": "^8.31.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "ng-packagr": "^19.2.2",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3"
  }
